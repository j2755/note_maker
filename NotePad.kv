ScreenManagement:
	id:manager

	Home:
	Other:


<Home>:
	name:"home"
	GridLayout:
		cols:1
		Label:
			text:'Notepad'
			size_hint: (.1, .1)
		TextInput:
			id: input
			text:'Text Input'
			pos:(100,100)
			
		Button:
			text: 'Submit'
			size_hint: (.2, .1)
			width: 500
			on_release: app.process()
		Button:
			text: 'Change to Other'
			on_release: app.root.current="other"
<Other>:
	name:'other'
	GridLayout:
		cols:1
		Label:
			text: 'Other Screen'
		Button:
			text: 'Change to home'
			on_release:app.root.current= 'home'